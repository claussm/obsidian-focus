{
  "name": "obsidian-focus",
  "version": "1.0.0",
  "description": "Aggregates and prioritizes todos from across your vault into a focused sidebar",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "todo",
    "focus"
  ],
  "author": "Matt",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.20.0",
    "obsidian": "latest",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  }
}
